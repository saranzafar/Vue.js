{
    "builds": [
        {
            "src": "client/vite.config.js",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "client/dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}